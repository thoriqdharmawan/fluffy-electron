{
  "name": "fluffy-electron",
  "description": "Fluffy electron print",
  "version": "1.0.0",
  "private": true,
  "author": "Thoriq Dharmawan",
  "main": "packages/main/dist/index.cjs",
  "scripts": {
    "build": "pnpm run build:main && pnpm run build:preload && pnpm run build:renderer",
    "build:main": "cd ./packages/main && vite build",
    "build:preload": "cd ./packages/preload && vite build",
    "build:renderer": "cd ./packages/renderer && vite build",
    "compile": "cross-env MODE=production pnpm run build && electron-builder build --config .electron-builder.config.js --dir --config.asar=false",
    "compile:win": "pnpm run compile --win portable",
    "test": "pnpm run test:main && pnpm run test:preload && pnpm run test:renderer && pnpm run test:e2e",
    "test:e2e": "pnpm run build && vitest run",
    "test:main": "vitest run -r packages/main --passWithNoTests",
    "test:preload": "vitest run -r packages/preload --passWithNoTests",
    "test:renderer": "vitest run -r packages/renderer --passWithNoTests",
    "watch": "node scripts/watch.mjs",
    "lint": "eslint . --ext js,mjs,cjs,ts,mts,cts,tsx",
    "lint:fix": "eslint . --ext js,mjs,cjs,ts,mts,cts,tsx --fix",
    "typecheck:main": "tsc --noEmit -p packages/main/tsconfig.json",
    "typecheck:preload": "tsc --noEmit -p packages/preload/tsconfig.json",
    "typecheck": "pnpm run typecheck:main && pnpm run typecheck:preload && pnpm run typecheck:renderer",
    "postinstall": "cross-env ELECTRON_RUN_AS_NODE=1 electron scripts/update-electron-vendors.mjs",
    "format": "pnpm dlx prettier --write \"**/*.{js,mjs,cjs,ts,mts,cts,tsx,json}\""
  },
  "devDependencies": {
    "@dohooo/eslint-config": "^0.0.7",
    "@types/react": "^17.0.38",
    "@types/react-dom": "^17.0.11",
    "@vitejs/plugin-react": "^1.1.3",
    "cross-env": "7.0.3",
    "electron": "21.3.0",
    "electron-builder": "23.6.0",
    "eslint": "8.27.0",
    "happy-dom": "7.7.0",
    "nano-staged": "0.8.0",
    "playwright": "1.27.1",
    "simple-git-hooks": "2.8.1",
    "typescript": "4.8.4",
    "unplugin-auto-expose": "0.0.4",
    "vite": "3.2.3",
    "vitest": "0.25.1"
  },
  "dependencies": {
    "@apollo/client": "^3.7.10",
    "@emotion/react": "^11.10.4",
    "@mantine/carousel": "5.9.0",
    "@mantine/core": "5.9.0",
    "@mantine/dates": "^5.10.5",
    "@mantine/dropzone": "^5.10.1",
    "@mantine/form": "^5.10.0",
    "@mantine/hooks": "5.9.0",
    "@mantine/modals": "^5.10.2",
    "@mantine/notifications": "5.9.0",
    "@node-escpos/core": "^0.0.3",
    "@node-escpos/usb-adapter": "^0.0.2",
    "@tabler/icons": "^1.107.0",
    "dayjs": "^1.11.7",
    "electron-updater": "5.3.0",
    "embla-carousel-react": "^7.0.3",
    "firebase": "^9.19.1",
    "graphql": "^16.6.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-loading-skeleton": "^3.2.0",
    "react-router-dom": "^6.9.0",
    "react-use-cart": "^1.13.0"
  }
}
